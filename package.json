    "@codemirror/view": "^6.7.3",
    "@headlessui/react": "^1.7.7",
    "@lezer/highlight": "^1.1.3",
    "clsx": "^1.2.1",
    "klona": "^2.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-use": "^17.4.0",
    "rust-optionals": "^3.0.0",
    "tdeep": "^0.1.0",
    "ts-pattern": "^4.1.3",
    "zustand": "^4.3.2"
