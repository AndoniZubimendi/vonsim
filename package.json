{
  "name": "vonsim",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "prettier -w .",
    "lint": "tsc --noEmit && eslint --ignore-path .gitignore --ext .js,.esm,.cjs,.ts,.tsx .",
    "bench": "vitest bench --run",
    "test": "vitest --run --reporter verbose",
